<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="mine_linear_styles">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">10dp</item>
    </style>
    <style name="mine_icon_styles">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>
    <style name="mine_title_styles">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18dp</item>
        <item name="android:layout_marginLeft">20dp</item>
    </style>
    <style name="mine_info_styles">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">right</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/dividerColor</item>
    </style>
    <style name="mine_small_divider_styles">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:background">@color/dividerColor</item>
    </style>
    <style name="mine_divider_styles">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:background">#ecebeb</item>
    </style>
</resources>